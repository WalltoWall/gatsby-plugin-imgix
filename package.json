{
  "name": "gatsby-plugin-imgix",
  "version": "0.8.2",
  "description": "Gatsby plugin which enables the use of Imgix to apply image transformations at request-time",
  "license": "BSD-2-Clause",
  "main": "dist/gatsby-plugin-imgix.js",
  "module": "dist/gatsby-plugin-imgix.modern.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "gatsby-node.js",
    "fragments.js"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "yarn clean && microbundle watch src/index.ts src/node.ts src/gatsby-node.ts -o dist -f modern,cjs",
    "build": "yarn clean && microbundle build src/index.ts src/node.ts src/gatsby-node.ts -o dist -f modern,cjs",
    "format": "prettier --write '{src,docs}/**/*.{ts,tsx,md}'",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "yarn build",
    "release": "standard-version"
  },
  "author": "Angelo Ashmore",
  "homepage": "https://github.com/WalltoWall/gatsby-plugin-imgix",
  "repository": {
    "type": "git",
    "url": "https://github.com/WalltoWall/gatsby-plugin-imgix.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "imgix",
    "images"
  ],
  "dependencies": {
    "camel-case": "^4.1.2",
    "fp-ts": "^2.9.3",
    "imgix-url-params": "^11.11.1",
    "md5": "^2.3.0",
    "node-fetch": "^2.6.1",
    "param-case": "^3.0.4"
  },
  "peerDependencies": {
    "gatsby": ">= 2.20.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^26.0.20",
    "@types/md5": "^2.2.1",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "gatsby": "^2.30.1",
    "gatsby-image": "^2.9.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 80
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
