{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src",
    "gatsby-node.js",
    "fragments.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production npm run clean && rollup -c",
    "format": "prettier --write '{src,docs,test,types}/**/*.{ts,tsx,md}'",
    "prepare": "npm run build",
    "release": "standard-version",
    "test": "jest"
  },
  "peerDependencies": {},
  "name": "gatsby-plugin-imgix",
  "author": "Angelo Ashmore",
  "dependencies": {
    "dlv": "^1.1.3",
    "fs-extra": "^9.0.0",
    "probe-image-size": "^5.0.0",
    "ts-imgix": "^0.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-json": "^4.0.2",
    "@types/dlv": "^1.1.2",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "cross-env": "^7.0.2",
    "gatsby": "^2.20.23",
    "gatsby-image": "^2.3.2",
    "husky": "^4.2.5",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "standard-version": "^7.1.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 80
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
