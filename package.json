{
  "name": "gatsby-plugin-imgix",
  "version": "0.5.0",
  "description": "Gatsby plugin which enables the use of Imgix to apply image transformations at request-time",
  "license": "MIT",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "gatsby-node.js",
    "fragments.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "clean": "rimraf ./lib",
    "build": "NODE_ENV=production yarn run clean && rollup -c",
    "test": "tsdx test",
    "lint": "eslint src/**/*.ts",
    "prepare": "tsdx build",
    "release": "standard-version"
  },
  "author": "Angelo Ashmore",
  "homepage": "https://github.com/WalltoWall/gatsby-plugin-imgix",
  "repository": {
    "type": "git",
    "url": "https://github.com/WalltoWall/gatsby-plugin-imgix.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "imgix",
    "images"
  ],
  "dependencies": {
    "camel-case": "^4.1.1",
    "fp-ts": "^2.6.1",
    "imgix-url-params": "^11.4.3",
    "node-fetch": "^2.6.0",
    "param-case": "^3.0.3"
  },
  "peerDependencies": {
    "gatsby": ">= 2.20.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/node-fetch": "^2.5.6",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "gatsby": "^2.20.24",
    "gatsby-image": "^2.3.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.9",
    "rollup-plugin-auto-external": "^2.0.0",
    "standard-version": "^7.1.0",
    "tsdx": "^0.13.2",
    "tslib": "^1.11.1",
    "typescript": "^3.9.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 80
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
